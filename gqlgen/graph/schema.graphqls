#https://gqlgen.com/getting-started/

#A Quote is a phrase. It is made up of id, quote and author
type Quote {
  #The id of a quote
  id: String!
  #The quote phrase
  quote: String!
  #The author a a quote
  author: String!
}


type Query {
  #get a random quote
  randomQuote: Quote!

  #get a quote by id
  quoteID(id: String!): Quote!
}
